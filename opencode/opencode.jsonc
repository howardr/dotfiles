{
  "$schema": "https://opencode.ai/config.json",
  "autoupdate": true,
  "permission": {
    "bash": {
      "*": "ask",
      "awk *": "allow",
      "basename *": "allow",
      "cat *": "allow",
      "column *": "allow",
      "cut *": "allow",
      "date": "allow",
      "date *": "allow",
      "df *": "allow",
      "dirname *": "allow",
      "du *": "allow",
      "echo *": "allow",
      "file *": "allow",
      "find *": "allow",
      "find *-exec*": "ask",
      "git blame *": "allow",
      "git branch": "allow",
      "git branch --contains *": "allow",
      "git branch --format *": "allow",
      "git branch --list": "allow",
      "git branch --list *": "allow",
      "git branch --show-current": "allow",
      "git branch -a": "allow",
      "git branch -l": "allow",
      "git branch -r": "allow",
      "git describe": "allow",
