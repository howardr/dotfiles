      "printf *": "allow",
      "ps *": "allow",
      "pwd": "allow",
      "rg *": "allow",
      "sed *": "allow",
      "sed --in-place": "ask",
      "sed --in-place *": "ask",
      "sed -i": "ask",
      "sed -i *": "ask",
      "sort *": "allow",
